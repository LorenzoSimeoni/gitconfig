[core]

[alias]
        a = add --all
        b = branch
        ba = branch -a
        bd = branch -d
        bD = branch -D
        c = commit
        cm = commit -m
        cd = commit --amend
        f = fetch
        fo = fetch origin
        fop = fetch origin -p
        m = merge
        ma = merge --abort
        mc = merge --continue
        ms = merge --skip
        ps = push
        pso = push origin
        pl = pull
        plo = pull origin
        prom = pull --rebase origin master
        rb = rebase
        rba = rebase --abort
        rbc = rebase --continue
        rbi = rebase --interactive
        s = status
        sb = status -s -b
        st = stash
        stl = stash list
        stp = stash pop
